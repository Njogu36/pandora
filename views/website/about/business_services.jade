                    .statement__block.section
                      .row__container
                        h3.large Get in touch now
                        p Enter your details below and we'll get back to you within the hour!
                        .form
                          script(type='text/javascript' src='https://form.jotformeu.com/jsform/82983668063369')
                          script(src='../-1960564991/Handler/CSSOverride/GetImage/2/autosize.min.js' type='text/javascript')
                          script.
                            (function() {
                            var FormLoaded = false;
                            $(window).load(function() {
                            var formParent = newjQuery(".form iframe");
                            var iframe = formParent.contents();
                            iframe.find("input").focus(function() {
                            $(this).closest(".form-line").addClass("active");
                            $(this).closest(".form-line").addClass("active");
                            });
                            iframe.find("input").blur(function() {
                            if ($(this).val() == "") {
                            $(this).closest(".form-line").removeClass("active");
                            $(this).closest(".form-line").removeClass("active");
                            }
                            });
                            iframe.find("textarea").focus(function() {
                            $(this).closest(".form-line").addClass("active");
                            $(this).closest(".form-line").addClass("active");
                            });
                            autosize(iframe.find("textarea"));
                            iframe.find("textarea").blur(function() {
                            if ($(this).val() == "") {
                            $(this).closest(".form-line").removeClass("active");
                            $(this).closest(".form-line").removeClass("active");
                            }
                            });
                            iframe.find(".formFooter").remove();
                            });
                            })();
