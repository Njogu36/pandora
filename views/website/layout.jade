    $(document).ready(function() {
    var newsLetterParent = $(".newsLetterSmallWrapper");
    var newsLetterInput = newsLetterParent.find("input#mce-EMAIL");
    var newsLetterTitle = newsLetterParent.find(".title div");
    $(newsLetterInput).focus(function() {
    newsLetterTitle.addClass("active");
    newsLetterInput.addClass("active");
    });
    $(newsLetterInput).focusout(function() {
    if (newsLetterInput.val() == "") {
    newsLetterTitle.removeClass("active");
    newsLetterInput.removeClass("active");
    }
    });
    $(newsLetterInput).keypress(function(event) {
    if (event.which == 13) {
    event.preventDefault();
    var subscribe = 'true';
    var emailValue = newsLetterParent.find(".Email").val();
    var subscribeProgress = newsLetterParent.find(".subscribe-loading-progress");
    var newsletterResult = newsLetterParent.find(".newsletter-result");
    subscribeProgress.show(); //Shows loading animation
    $.ajax({
    cache: false,
    type: "POST",
    url: "/Newsletter/SubscribeNewsletter",
    data: "subscribe=" + subscribe + "&email=" + emailValue,
    success: function(data) {
    subscribeProgress.hide();
    newsletterResult.html(data.Result);
    if (data.Success) {
    newsletterResult.fadeIn();
    newsLetterTitle.removeClass("active");
    newsLetterParent.addClass("submitted");
    if (data.Result.indexOf("already subscribed") > -1) {
    } else {
    newsLetterParent.find('.formItems').fadeOut();
    }
    } else {
    newsletterResult.fadeIn("slow").delay(2000).fadeOut("slow");
    }
    },
    error: function(xhr, ajaxOptions, thrownError) {
    alert('Failed to subscribe.');
    subscribeProgress.hide();
    }
    });
    }
    });
    //Cleate Sticky element
    if ($('.sticky').length > 0) {
    $('.sticky').Stickyfill();
    }
    //Create Customcheckboxes
    if (Modernizr.testProp('pointerEvents')) { //Pointer events are disabled with this trick
    function createCustomCheckboxes(elements) {
    $(elements).addClass("updated");
    $.each(elements, function(index, item) {
