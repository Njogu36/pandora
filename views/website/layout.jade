      //Replace search submit function
      function check_search_form() {
      if ($(".searchbox_wrapper.show").length <= 0) {
      $(".searchbox_wrapper").addClass("show");
      setTimeout(function() {
      $("#small-searchterms").focus();
      }, 500);
      return false;
      } else {
      var search_terms = $("#small-searchterms");
      if (search_terms.val() == "" || search_terms.val() == 'Search products') {
      alert('Please enter some search keyword');
      $(".searchbox_wrapper").addClass("show");
      setTimeout(function() {
      $("#small-searchterms").focus();
      }, 500);
      return false;
      }
      var countChar = search_terms.val().length;
      if (countChar <= 2) {
      alert('Search keyword must contain more than three characters');
      $(".searchbox_wrapper").addClass("show");
      setTimeout(function() {
      $("#small-searchterms").focus();
      }, 500);
      return false;
      }
      return true;
      }
      }
