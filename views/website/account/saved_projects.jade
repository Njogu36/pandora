                    script(type='text/javascript').
                      $(document).ready(function() {
                      $('#searchSavedProjects').click(function() {
                      var grid = $('#SavedProjects').data('tGrid');
                      grid.currentPage = 1; //new search. Set page size to 1
                      grid.ajaxRequest();
                      return false;
                      });
                      $("#searchValue").keydown(function(event) {
                      if (event.keyCode == 13) {
                      $("#searchSavedProjects").click();
                      return false;
                      }
                      });
                      });
                      function onDataBinding(e) {
                      var searchModel = {
                      searchSavedProject: $('#searchValue').val()
                      };
                      e.data = searchModel;
                      }
