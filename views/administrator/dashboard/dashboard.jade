extend ../layout.jade 
block content
 .content-wrapper
            
            .container-xxl.flex-grow-1.container-p-y
              .row
                // Website Analytics
                
                .col-lg-3.col-md-6.col-sm-6.mb-4
                  .card
                    .card-body.pb-0
                      .card-icon
                        span.badge.bg-label-success.rounded-pill.p-2
                          i.ti.ti-credit-card.ti-sm
                      h5.card-title.mb-0.mt-2 97.5k
                      small Revenue Generated.
                    #revenueGenerated
                // / Revenue Generated
                // Earning Reports
                
                
                // Sales By Country
                
                .col-12.col-xl-4.mb-4.col-md-6
                  .card
                    .card-header.d-flex.justify-content-between.pb-1
                      h5.mb-0.card-title Total Earning
                      .dropdown
                        button#totalEarning.btn.p-0(type='button' data-bs-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                          i.ti.ti-dots-vertical.ti-sm.text-muted
                        .dropdown-menu.dropdown-menu-end(aria-labelledby='totalEarning')
                          a.dropdown-item(href='javascript:void(0);') View More
                          a.dropdown-item(href='javascript:void(0);') Delete
                    .card-body
                      .d-flex.align-items-center
                        h1.mb-0.me-2 87%
                        i.ti.ti-chevron-up.text-success.me-1
                        p.text-success.mb-0 25.8%
                      #totalEarningChart
                      .d-flex.align-items-start.my-4
                        .badge.rounded.bg-label-primary.p-2.me-3.rounded
                          i.ti.ti-currency-dollar.ti-sm
                        .d-flex.justify-content-between.w-100.gap-2.align-items-center
                          .me-2
                            h6.mb-0 Total Sales
                            small.text-muted Refund
                          p.mb-0.text-success +$98
                      .d-flex.align-items-start
                        .badge.rounded.bg-label-secondary.p-2.me-3.rounded
                          i.ti.ti-brand-paypal.ti-sm
                        .d-flex.justify-content-between.w-100.gap-2.align-items-center
                          .me-2
                            h6.mb-0 Total Revenue
                            small.text-muted Client Payment
                          p.mb-0.text-success +$126
                // / Total Earning
                // Monthly Campaign State
                
                .col-xl-4.col-md-6.order-2.order-lg-1
                  .card
                    .card-header.d-flex.justify-content-between
                      .card-title.mb-0
                        h5.mb-0 Website Visits
                        small.text-muted 38.4k Visitors
                      .dropdown
                        button#sourceVisits.btn.p-0(type='button' data-bs-toggle='dropdown' aria-haspopup='true' aria-expanded='false')
                          i.ti.ti-dots-vertical.ti-sm.text-muted
                        .dropdown-menu.dropdown-menu-end(aria-labelledby='sourceVisits')
                          a.dropdown-item(href='javascript:void(0);') Refresh
                          a.dropdown-item(href='javascript:void(0);') Download
                          a.dropdown-item(href='javascript:void(0);') View All
                    .card-body
                      ul.list-unstyled.mb-0
                        li.mb-3.pb-1
                          .d-flex.align-items-start
                            .badge.bg-label-secondary.p-2.me-3.rounded
                              i.ti.ti-shadow.ti-sm
                            .d-flex.justify-content-between.w-100.flex-wrap.gap-2
                              .me-2
                                h6.mb-0 Direct Source
                                small.text-muted Direct link click
                              .d-flex.align-items-center
                                p.mb-0 1.2k
                                .ms-3.badge.bg-label-success +4.2%
                        li.mb-3.pb-1
                          .d-flex.align-items-start
                            .badge.bg-label-secondary.p-2.me-3.rounded
                              i.ti.ti-globe.ti-sm
                            .d-flex.justify-content-between.w-100.flex-wrap.gap-2
                              .me-2
                                h6.mb-0 Social Network
                                small.text-muted Social Channels
                              .d-flex.align-items-center
                                p.mb-0 31.5k
                                .ms-3.badge.bg-label-success +8.2%
                        li.mb-3.pb-1
                          .d-flex.align-items-start
                            .badge.bg-label-secondary.p-2.me-3.rounded
                              i.ti.ti-mail.ti-sm
                            .d-flex.justify-content-between.w-100.flex-wrap.gap-2
                              .me-2
                                h6.mb-0 Email Newsletter
                                small.text-muted Mail Campaigns
                              .d-flex.align-items-center
                                p.mb-0 893
                                .ms-3.badge.bg-label-success +2.4%
                        li.mb-3.pb-1
                          .d-flex.align-items-start
                            .badge.bg-label-secondary.p-2.me-3.rounded
                              i.ti.ti-external-link.ti-sm
                            .d-flex.justify-content-between.w-100.flex-wrap.gap-2
                              .me-2
                                h6.mb-0 Referrals
                                small.text-muted Impact Radius Visits
                              .d-flex.align-items-center
                                p.mb-0 342
                                .ms-3.badge.bg-label-danger -0.4%
                        li.mb-3.pb-1
                          .d-flex.align-items-start
                            .badge.bg-label-secondary.p-2.me-3.rounded
                              i.ti.ti-discount-2.ti-sm
                            .d-flex.justify-content-between.w-100.flex-wrap.gap-2
                              .me-2
                                h6.mb-0 ADVT
                                small.text-muted Google ADVT
                              .d-flex.align-items-center
                                p.mb-0 2.15k
                                .ms-3.badge.bg-label-success +9.1%
                        li.mb-2
                          .d-flex.align-items-start
                            .badge.bg-label-secondary.p-2.me-3.rounded
                              i.ti.ti-star.ti-sm
                            .d-flex.justify-content-between.w-100.flex-wrap.gap-2
                              .me-2
                                h6.mb-0 Other
                                small.text-muted Many Sources
                              .d-flex.align-items-center
                                p.mb-0 12.5k
                                .ms-3.badge.bg-label-success +6.2%
                // / Source Visit
                // Projects table
                
                // / Projects table
                // / Content
                // Footer
                
                .content-backdrop.fade
          